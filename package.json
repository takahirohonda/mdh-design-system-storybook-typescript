{
  "name": "@mydatahack/mdh-design-system-storybook",
  "version": "1.6.0",
  "description": "Mdh Design System with Storybook",
  "main": "dist/index.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": "git@github.com:mydatahack/mdh-design-system-storybook-typescript.git",
  "scripts": {
    "build:package": "yarn run clean ; rollup -c rollup.config.js",
    "analyse:package": "yarn run clean ; export ANALYZE=true || set ANALYZE=true&&rollup -c rollup.config.js",
    "clean": "rimraf dist",
    "commit": "cz",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "devbuild": "webpack --mode development",
    "build": "webpack --mode production",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint-css": "stylelint 'src/**/*.{c,sc,le}ss'",
    "fix-css": "printf '\\033[43mNote: You may need to run this again if any of your CSS property names contain uppercase letters.\\033[0m' && stylelint 'src/**/*.{c,sc,le}ss' --fix -f 'verbose'",
    "lint-js": "eslint 'src/**/*.{ts,tsx,js,jsx}' --quiet",
    "fix-js": "eslint 'src/**/*.{ts,tsx,js,jsx}' --fix --quiet",
    "check-types": "tsc --noEmit"
  },
